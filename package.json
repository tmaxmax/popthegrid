{
  "name": "popthegrid",
  "version": "0.4.5",
  "description": "Frontend for the 'Pop the grid!' game",
  "type": "module",
  "scripts": {
    "dev:funcs": "air -c .air.toml",
    "dev:static": "parcel src/index.html",
    "dev": "run-p dev:*",
    "build:static": "parcel build src/index.html",
    "build:funcs": "go build -o ./.netlify/functions ./cmd/...",
    "build": "run-s build:*",
    "test": "jest --passWithNoTests --maxWorkers=1",
    "prebuild": "[[ $CI = true ]] && npx pnpm@3 install -r --store=node_modules/.pnpm-store || echo skiping pnpm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tmaxmax/popthegrid.git"
  },
  "author": "Teodor Maxim",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tmaxmax/popthegrid/issues"
  },
  "homepage": "https://github.com/tmaxmax/popthegrid#readme",
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.20",
    "@types/jsdom": "^16.2.6",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "cssnano": "^4.1.10",
    "eslint": "^7.21.0",
    "eslint-plugin-jsdoc": "^32.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-websocket-mock": "^2.2.0",
    "jsdom": "^16.4.0",
    "mock-socket": "^9.0.3",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "1.12.3",
    "postcss": "^8.2.7",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.3",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "debug": "^4.3.1",
    "ms": "^2.1.3",
    "resize-observer-polyfill": "^1.5.1",
    "supports-color": "^8.1.1"
  },
  "main": "src/index.ts"
}
