{
  "arrowParens": "always",
  "bracketSpacing": true,
  "endOfLine": "lf",
  "semi": false,
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "printWidth": 140,
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "semi": true,
        "svelteSortOrder": "scripts-markup-styles-options",
        "svelteAllowShorthand": true,
        "svelteBracketNewLine": false,
        "svelteIndentScriptAndStyle": true
      }
    }
  ]
}
